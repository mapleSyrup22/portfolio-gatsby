{
  "kind": "singleType",
  "collectionName": "displays",
  "info": {
    "name": "Contact Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "mobileNumber": {
      "type": "string",
      "regex": "^\\+\\d{3,}"
    },
    "email": {
      "type": "email"
    },
    "github": {
      "type": "string",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "lat": {
      "type": "float",
      "required": true
    },
    "lng": {
      "required": true,
      "type": "float"
    }
  }
}
